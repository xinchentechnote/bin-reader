{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug bin-reader",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/bin-reader",
            "args": [
                "-f",
                "tests/data/HelloWorld.java"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "CMake Build"
        }
    ]
}